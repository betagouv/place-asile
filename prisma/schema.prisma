generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Structure {
  id                    Int     @id @default(autoincrement())
  operateur             String
  type                  String
  nbPlaces              Int
  adresseHebergement    String
  codePostalHebergement String
  communeHebergement    String
  nbHebergements        Int
  typologie             String
  latitude              Decimal
  longitude             Decimal
}