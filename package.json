{
  "name": "place-asile",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22",
    "yarn": "1.22"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx @tailwindcss/cli -i ./src/app/globals.css -o build.css --minify && next build && npx tsx copy-assets.ts",
    "start": "node .next/standalone/server.js",
    "check:ts": "npx tsc --noEmit",
    "lint": "yarn lint:css && yarn lint:ts",
    "lint:ts": "next lint",
    "lint:css": "npx stylelint \"**/*.css\"",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "prepare": "husky",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio",
    "prisma:reset": "npx prisma migrate reset",
    "predev": "react-dsfr update-icons --silent",
    "prebuild": "react-dsfr update-icons",
    "postinstall": "npx patch-package"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.26.0",
    "@faker-js/faker": "^9.9.0",
    "@formkit/auto-animate": "^0.8.2",
    "@gouvfr/dsfr-chart": "1.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.13.0",
    "@sentry/nextjs": "^9.42.0",
    "@socialgouv/matomo-next": "^1.9.2",
    "@tailwindcss/postcss": "^4.1.11",
    "chartist": "^1.4.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "leaflet": "^1.9.4",
    "minio": "^8.0.5",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "next-leaflet-cluster": "^1.0.0",
    "postcss": "^8.5.6",
    "pretty-bytes": "^7.0.0",
    "prisma": "^6.13.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "^5.0.0",
    "read-excel-file": "^5.8.8",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.74 "
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "node-mocks-http": "^1.17.2",
    "node-xlsx": "^0.24.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "sass": "^1.90.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^38.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
